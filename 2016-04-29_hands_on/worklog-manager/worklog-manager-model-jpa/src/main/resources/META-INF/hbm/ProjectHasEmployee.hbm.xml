<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="at.fh.ooe.swt6.worklog.manager.model.jpa.ProjectHasEmployee" table="project_has_employee">
        <composite-id name="id" class="at.fh.ooe.swt6.worklog.manager.model.jpa.ProjectHasEmployeeId">
            <key-property name="projectId">
                <column name="project_id"/>
            </key-property>
            <key-property name="employeeId">
                <column name="employee_id"/>
            </key-property>
        </composite-id>
        <property name="version">
            <column name="version" sql-type="bigint(20)"/>
        </property>
        <property name="creationDate">
            <column name="creationDate" sql-type="datetime"/>
        </property>
        <property name="modificationDate">
            <column name="modificationDate" sql-type="datetime"/>
        </property>
        <many-to-one name="employee" class="at.fh.ooe.swt6.worklog.manager.model.jpa.Employee" insert="false" update="false">
            <column name="employee_id"/>
        </many-to-one>
        <many-to-one name="project" class="at.fh.ooe.swt6.worklog.manager.model.jpa.Project" insert="false" update="false">
            <column name="project_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>