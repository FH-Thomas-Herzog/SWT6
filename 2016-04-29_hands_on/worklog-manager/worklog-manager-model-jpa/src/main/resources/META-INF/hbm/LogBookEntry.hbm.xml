<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="at.fh.ooe.swt6.worklog.manager.model.jpa.LogBookEntry" table="log_book_entry">
        <id name="id">
            <column name="id" sql-type="bigint(20)"/>
        </id>
        <property name="version">
            <column name="version" sql-type="bigint(20)"/>
        </property>
        <property name="creationDate">
            <column name="creationDate" sql-type="datetime"/>
        </property>
        <property name="modificationDate">
            <column name="modificationDate" sql-type="datetime"/>
        </property>
        <property name="activity">
            <column name="activity" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="endTime">
            <column name="endTime" sql-type="time"/>
        </property>
        <property name="startTime">
            <column name="startTime" sql-type="time"/>
        </property>
        <many-to-one name="phase" class="at.fh.ooe.swt6.worklog.manager.model.jpa.Phase">
            <column name="phase_id"/>
        </many-to-one>
        <many-to-one name="employee" class="at.fh.ooe.swt6.worklog.manager.model.jpa.Employee">
            <column name="employee_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>